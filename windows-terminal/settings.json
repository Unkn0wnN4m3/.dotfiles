{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": "togglePaneZoom",
      "keys": "alt+shift+z"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "right",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+p"
    },
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+shift+c"
    },
    {
      "command": "paste",
      "keys": "ctrl+shift+v"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "up"
      },
      "keys": "alt+shift+k"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "right"
      },
      "keys": "alt+l"
    },
    {
      "command": "find",
      "keys": "ctrl+shift+f"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "up"
      },
      "keys": "alt+k"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "right"
      },
      "keys": "alt+shift+l"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "down"
      },
      "keys": "alt+j"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "left"
      },
      "keys": "alt+shift+h"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "down",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+o"
    },
    {
      "command": {
        "action": "moveFocus",
        "direction": "left"
      },
      "keys": "alt+h"
    },
    {
      "command": {
        "action": "resizePane",
        "direction": "down"
      },
      "keys": "alt+shift+j"
    }
  ],
  "copyFormatting": "none",
  "copyOnSelect": true,
  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "launchMode": "default",
  "profiles": {
    "defaults": {
      "bellStyle": "window",
      "colorScheme": "Campbell",
      "opacity": 90,
      "padding": "1",
      "useAcrylic": true
    }
  },
  "schemes": [
    {
      "background": "#0C0C0C",
      "black": "#282C34",
      "blue": "#61AFEF",
      "brightBlack": "#5A6374",
      "brightBlue": "#61AFEF",
      "brightCyan": "#56B6C2",
      "brightGreen": "#98C379",
      "brightPurple": "#C678DD",
      "brightRed": "#E06C75",
      "brightWhite": "#DCDFE4",
      "brightYellow": "#E5C07B",
      "cursorColor": "#FFFFFF",
      "cyan": "#56B6C2",
      "foreground": "#DCDFE4",
      "green": "#98C379",
      "name": "One Half Dark (modded)",
      "purple": "#C678DD",
      "red": "#E06C75",
      "selectionBackground": "#FFFFFF",
      "white": "#DCDFE4",
      "yellow": "#E5C07B"
    },
    {
      "background": "#002B36",
      "black": "#073642",
      "blue": "#268BD2",
      "brightBlack": "#475B62",
      "brightBlue": "#708284",
      "brightCyan": "#819090",
      "brightGreen": "#475B62",
      "brightPurple": "#6C71C4",
      "brightRed": "#CB4B16",
      "brightWhite": "#FCF4DC",
      "brightYellow": "#536870",
      "cursorColor": "#708284",
      "cyan": "#2AA198",
      "foreground": "#EEE8D5",
      "green": "#859900",
      "name": "Solarized Dark - Patched",
      "purple": "#D33682",
      "red": "#DC322F",
      "selectionBackground": "#002831",
      "white": "#EAE3CB",
      "yellow": "#B58900"
    }
  ],
  "trimBlockSelection": true,
  "useAcrylicInTabRow": true
}
